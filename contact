<div class="content_container main_container">
    <h4 class="promo_main_header">Contact Us</h3>
    <div class="contact_container">
        <div class="row">
            <div class="col-md-8 contact_contents">
                <div class="overlay2" onclick="style.pointerEvents='none'"></div>
                <iframe class="direction_map" src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d12382.058905971786!2d-77.200455!3d39.117517!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x31b02cbfb23cdc39!2sRIO+Washingtonian+Center!5e0!3m2!1sen!2sus!4v1477497507151" width="100%" height="400" frameborder="0" style="border:0" allowfullscreen></iframe>
                <div id="mm_page_content" class="promo_list_desc"></div>
            </div>
            <div class="col-md-4 hidden_phone">
            </div> 
        </div>
    </div>
</div>
<script>
    $(document).ready(function(e){
        init(e);
        loadMallDataCached(renderPage);
    })
    
    function renderPage(){
        var pathArray = window.location.pathname.split( '/' );
        var slug = pathArray[pathArray.length-1];
        
        prefix = get_prefix();
        $('#top_banner').addClass(slug)
        var pages_json = prefix+"/pages/washingtonian-contact-us.json"
        $.getJSON(pages_json).done(function(data) {
            $('#mm_page_content').html(data.body);
        }).fail(function(jqXHR) {
            if (jqXHR.status == 404) {
                $("#mm_page_content").text("Sorry. We cant find the page you looking for.");
            }
        }); 
        show_content();
    }
</script>